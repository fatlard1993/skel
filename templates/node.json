{
	"${name}": {
		"src": {
			"index.js": "#!/usr/bin/env node\n"
		},
		".gitignore": "**/node_modules\n\ntemp",
		".npmrc": "package-lock=false",
		"package.json": {
			"_jsonFile": true,
			"name": "${name}",
			"version": "1.0.0",
			"main": "src/index.js",
			"bin": "src/index.js",
			"description": "${name}",
			"author": "${gitHubUser}",
			"license": "ISC",
			"scripts": {
				"test": "echo \"Error: no test specified\" && exit 1"
			},
			"repository": {
				"type": "git",
				"url": "git+https://github.com/${gitHubUser}/${name}.git"
			},
			"bugs": {
				"url": "https://github.com/${gitHubUser}/${name}/issues"
			},
			"homepage": "https://github.com/${gitHubUser}/${name}#readme",
			"eslintConfig": {
				"extends": "rules"
			},
			"devDependencies": {
				"eslint-config-rules": "github:${gitHubUser}/eslint-config-rules#2.x.x",
				"babel-eslint": "^10.0.3",
				"eslint": "^6.6.0"
			},
			"dependencies": {
				"log": "github:fatlard1993/log#2.x.x"
			}
		}
	}
}