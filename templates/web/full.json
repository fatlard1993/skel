{
	"_opts": "name",
	"_scripts": ["node/init"],
	"${name}": {
		"client": {
			"css": {
				"index.css": "// includes dom\n"
			},
			"html": {
				"head.html": ["web/client/head.js"],
				"index.html": ["web/client/index.js"]
			},
			"js": {
				"index.js": ["web/client/full/index.js"]
			},
			"fonts": {},
			"resources": {
				"site.webmanifest": {
					"_jsonFile": true,

					"lang": "en-US",
					"orientation": "any",
					"display": "standalone",
					"theme_color": "#BADA55",
					"background_color": "#000",
					"name": "${name}",
					"short_name": "${name}",
					"start_url": "/home"
				}
			}
		},
		"server": {
			"index.js": ["node/index.js"],
			"${camelName}.js": ["web/node/main.js"]
		},
		".gitignore": "**/node_modules\n\ntemp",
		".npmrc": "package-lock=false",
		"README.md": ["README.md"],
		"package.json": ["web/node/package.json"]
	}
}